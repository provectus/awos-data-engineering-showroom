# Weather-Bike-Demo Environment Variables
# Copy this file to .env and customize as needed

# DuckDB Configuration
DUCKDB_PATH=./duckdb/warehouse.duckdb

# NYC Coordinates for Weather API
NYC_LATITUDE=40.73
NYC_LONGITUDE=-73.94

# Data Date Ranges
BIKE_START_MONTH=2024-05
BIKE_END_MONTH=2024-06
WEATHER_START_DATE=2024-05-01
WEATHER_END_DATE=2024-06-30

# Citi Bike Data Source
CITIBIKE_BASE_URL=https://s3.amazonaws.com/tripdata

# Airflow Configuration
AIRFLOW_HOME=./airflow
AIRFLOW__CORE__DAGS_FOLDER=./airflow/dags
AIRFLOW__CORE__LOAD_EXAMPLES=False
AIRFLOW__CORE__EXECUTOR=LocalExecutor

# dbt Configuration
DBT_PROFILES_DIR=./dbt
DBT_PROJECT_DIR=./dbt

# Streamlit Configuration
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=localhost

